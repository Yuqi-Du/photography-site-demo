<h1 class="pb-4">Explore Latest</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  <% if(photo != '' && photo )   { %>
    <% photo.forEach(function(photo, index){ %>
    <a href="/photo/<%= photo._id %>" class="col text-center category__link">
      <div class="category__img category__img--large shadow">
        <img src="/uploads/<%= photo.image %>" alt="<%= photo.name %>" loading="lazy">
      </div>
      <div class="pt-1"><%= photo.name %></div>
    </a>
    <% }) %>
  <% } else { %>
    <p>No items found.</p>
  <% } %> 
</div>